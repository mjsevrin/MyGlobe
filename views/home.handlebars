<div class="dark content">
  <div id="homeCarousel" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
      <li data-target="#homeCarousel" data-slide-to="0" class="active"></li>
      <li data-target="#homeCarousel" data-slide-to="1"></li>
      <li data-target="#homeCarousel" data-slide-to="2"></li>
      <li data-target="#homeCarousel" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner">
      <div class="carousel-item">
        <img class="d-block w-100" src="/images/banner/lake.jpg">
      </div>
      <div class="carousel-item active">
        <img class="d-block w-100" src="/images/banner/mountain.jpg">
      </div>
      <div class="carousel-item">
        <img class="d-block w-100" src="/images/banner/beach.jpg">
      </div>
      <div class="carousel-item">
        <img class="d-block w-100" src="/images/banner/urban.jpg">
      </div>
    </div>
    <a class="carousel-control-prev" href="#homeCarousel" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#homeCarousel" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</div>

{{>small_footer}}

<script>
    $('.carousel').carousel({
        pause: false,
        interval: 5000
    })
</script>
